{
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch via yarn",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev:debug"],
      "skipFiles": ["<node_internals>/**"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch via npm",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev:debug"],
      "skipFiles": ["<node_internals>/**"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
